@model IEnumerable<CVOIS.Models.Admin.Departments>

@{
    ViewData["Title"] = "Departments";
}
<h1>View Departments</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ministry_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeptName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dept_status)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.SNo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Ministry_Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DeptName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Dept_status)
                </td>
            </tr>
        }
    </tbody>
</table>
